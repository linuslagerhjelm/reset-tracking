{
  "name": "Reset tracking",
  "version": "1.0",
  "description": "Clears local data of the current website",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "browsingData",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "resources/icon-16.png",
      "32": "resources/icon-32.png"
    },
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}